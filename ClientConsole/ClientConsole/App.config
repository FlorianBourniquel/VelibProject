                contract="ServiceReferenceVelib.IVelib" name="BasicHttpBinding_IVelib" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WcfServiceLibraryVelib/ServiceEvent/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ISubEvent"
                contract="EventStation.ISubEvent" name="WSDualHttpBinding_ISubEvent">
